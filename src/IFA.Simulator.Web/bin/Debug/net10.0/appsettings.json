{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "IFA": {
    "EventIntervalMs": 2000,
    "MaxRecentEventsDisplay": 50,
    "EnableEmailChannel": true,
    "EnableVmsChannel": true,
    "EnableImChannel": true,
    "EnableWebClientChannel": true,
    "LogDirectory": "logs"
  }
}
